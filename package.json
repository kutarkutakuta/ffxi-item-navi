{
  "name": "ffxi-item-navi",
  "version": "0.0.0",
  "engines": {
    "node": "16.15.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-hashing=all",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "create-public": "run-script-os",
    "create-public:darwin:linux": "ln -s -f dist/ffxi_item_navi/browser public",
    "create-public:win32": "",
    "copy1": "cpx ./src/privacy.html ./dist/ffxi_item_navi/browser",
    "copy2": "cpx ./src/.well-known/assetlinks.json ./dist/ffxi_item_navi/browser/.well-known/",
    "copy3": "cpx ./src/robots.txt ./dist/ffxi_item_navi/browser",
    "copy4": "cpx ./src/sitemap.xml ./dist/ffxi_item_navi/browser",
    "copy5": "cpx ./dist/ffxi_item_navi/browser/index.html ./dist/ffxi_item_navi/browser/list",
    "copy6": "cpx ./dist/ffxi_item_navi/browser/index.html ./dist/ffxi_item_navi/browser/myset",
    "clean1": "rm ./dist/ffxi_item_navi/browser/favicon.ico",
    "preinstall": "npm i run-script-os && npm run create-public",
    "postbuild": "ng run ffxi_item_navi:app-shell && npm run copy1 && npm run copy2 && npm run copy3 && npm run copy4 && npm run copy5 && npm run copy6 && npm run clean1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.2",
    "@angular/cdk": "^15.1.5",
    "@angular/common": "^15.1.2",
    "@angular/compiler": "^15.1.2",
    "@angular/core": "^15.1.2",
    "@angular/forms": "^15.1.2",
    "@angular/platform-browser": "^15.1.2",
    "@angular/platform-browser-dynamic": "^15.1.2",
    "@angular/platform-server": "^15.1.2",
    "@angular/router": "^15.1.2",
    "@angular/service-worker": "^15.1.2",
    "@supabase/supabase-js": "^2.2.3",
    "cpx": "^1.5.0",
    "dexie": "^3.2.3",
    "json-cyclic": "^1.0.2",
    "moment": "^2.29.4",
    "ng-zorro-antd": "^15.0.3",
    "primeflex": "^3.3.0",
    "rimraf": "^4.4.1",
    "run-script-os": "^1.1.6",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.2",
    "@angular/cli": "~15.1.2",
    "@angular/compiler-cli": "^15.1.2",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.2"
  }
}
